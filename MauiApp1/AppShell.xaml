<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1"
    Shell.FlyoutBehavior="Flyout"
    Shell.NavBarHasShadow="True"
    Shell.BackgroundColor="LightGrey"
    Shell.ForegroundColor="Black"
    Shell.TitleColor="Black"
    Shell.DisabledColor="Grey"
    >
    <ShellContent Title="Go To Country Page 1" Route="country1" ContentTemplate="{DataTemplate local:CountryPage1}" />
    <ShellContent Title="Go To Country Page 2" Route="country2" ContentTemplate="{DataTemplate local:CountryPage2}" />
    <ShellContent Title="Go To Country Page 3" Route="country3" ContentTemplate="{DataTemplate local:CountryPage3}" />
    <ShellContent Title="Go To Country Page 4" Route="country4" ContentTemplate="{DataTemplate local:CountryPage4}" />
</Shell>
